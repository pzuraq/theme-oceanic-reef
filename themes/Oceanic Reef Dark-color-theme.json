	"tokenColors": [
		{
			"settings": {
				"foreground": "#efefefff",
				"background": "#12171fff"
			}
		},
		{
			"name": "Comments",
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#65737e"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"foreground": "#FCC672"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#E47D80"
			}
		},
		{
			"name": "Storage",
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#D95757"
			}
		},
		{
			"name": "Storage",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FFB266"
			}
		},
		{
			"name": "Operator",
