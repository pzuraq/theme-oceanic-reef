		"widget.shadow": "#000000",
		"input.background": "#ffffff",
		"input.foreground": "#222222",
		"inputOption.activeBorder": "#d05572",
		"dropdown.background": "#203f54",
		"list.activeSelectionBackground": "#d05572",
		"button.background": "#d05572",
		"button.hoverBackground": "#a5455c",
		"scrollbar.shadow": "#000000",
		"editor.background": "#0d2839",
		"editor.foreground": "#f8f8f2",
		"editorWidget.background": "#0c2433",
		"editor.selectionBackground": "#2e4553",
		"editorHoverWidget.background": "#0c2433",
